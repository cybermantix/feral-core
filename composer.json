{
    "name": "noloco/core",
    "description": "This package provides a powerful core for No Code, Low Code solutions.",
    "keywords": ["no-code", "low-code", "bpms"],
    "homepage": "https://noloco.net",
    "type": "library",
    "license": "Apache License 2.0",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "psr/event-dispatcher": "^1.0",
        "psr/log": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "NoLoCo\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NoLoCo\\Core\\Tests\\": "tests/"
        }
    }
}
